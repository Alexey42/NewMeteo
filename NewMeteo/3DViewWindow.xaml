<Window x:Class="NewMeteo._3DViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:helix="http://helix-toolkit.org/wpf"
        xmlns:local="clr-namespace:NewMeteo"
        mc:Ignorable="d"
        Title="_3DViewWindow" Height="450" Width="800">
    <Grid>
        <Menu Height="22" VerticalAlignment="Top">
            <MenuItem Header="Save" Click="SaveSTL"></MenuItem>
        </Menu>

        <helix:HelixViewport3D x:Name="viewPort3d" Margin="0,22,0,0">
            <helix:DefaultLights />
            
        </helix:HelixViewport3D>
        
    </Grid>
</Window>
