<Window x:Class="NewMeteo.SetSmoothingCoefsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewMeteo"
        mc:Ignorable="d"
        Title="SetSmoothingCoefsWindow" Height="300" Width="280">
    <Grid>
        <Button x:Name="button" Click="Enter" IsDefault="True" Content="Ok" HorizontalAlignment="Center" Margin="0,234,0,0" VerticalAlignment="Top" Width="70" Height="25" FontFamily="Arial" FontSize="16" />
        <TextBox x:Name="rangeCoef" HorizontalAlignment="Left" Margin="84,158,0,0" Text="0,6" TextWrapping="Wrap" VerticalAlignment="Top" Width="40"/>
        <TextBox x:Name="decreaseCoef" HorizontalAlignment="Left" Margin="172,158,0,0" Text="0,8" TextWrapping="Wrap" VerticalAlignment="Top" Width="40"/>
        <Label Content="Range" HorizontalAlignment="Left" Margin="82,116,0,0" VerticalAlignment="Top"/>
        <Label Content="Decrease" HorizontalAlignment="Left" Margin="163,116,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
